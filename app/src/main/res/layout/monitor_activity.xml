                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="Server Connection: "
                android:textSize="15dp" />

            <TextView
                android:id="@+id/Ocpp_Status_Set"
                android:layout_width="210dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="170dp"
                android:text=""
                android:textColor="#558cff"
                android:textSize="15dp" />
            <Button
                android:id="@+id/Ocpp_online"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignRight="@id/Ocpp_Status_Set"
                />



            <Button
                android:id="@+id/Ocpp_not_offline"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignRight="@id/Ocpp_Status_Set"
                 />

            <Button
                android:id="@+id/Ocpp_downloading"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignRight="@id/Ocpp_Status_Set"
                />

            <Button
                android:id="@+id/Ocpp_offline"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignRight="@id/Ocpp_Status_Set"
               />



